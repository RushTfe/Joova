<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>


<HBox id="tableHboxRoot" fx:id="presentacionRoot" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView id="visitasTable" fx:id="presentacionTable" fixedCellSize="0.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
         <columns>
            <TableColumn id="nombreColumn" fx:id="nombreColumn" prefWidth="75.0" text="Nombre" />
            <TableColumn id="direccionColumn" fx:id="direccionColumn" prefWidth="75.0" text="Dirección" />
            <TableColumn id="fechaColumn" fx:id="fechaColumn" prefWidth="75.0" text="Fecha" />
            <TableColumn id="huboVentaColumn" fx:id="ventaColumn" prefWidth="75.0" text="¿Hubo Venta?" />
         </columns>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
