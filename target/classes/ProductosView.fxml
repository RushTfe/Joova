<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="rootProductos" prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <ListView fx:id="cajonProductosListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
   </center>
   <bottom>
      <HBox alignment="CENTER" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="anadirProductoButton" mnemonicParsing="false" text="+" />
            <Button fx:id="eliminarProductoButton" mnemonicParsing="false" text="Elim" />
            <Button fx:id="modificarProductoButton" mnemonicParsing="false" text="Modif" />
            <Button fx:id="anadirFiltroButton" mnemonicParsing="false" text="filtros" />
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
